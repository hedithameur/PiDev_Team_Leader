<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pidev.gui.AffichertableauController">
   <children>
      <TableView fx:id="tableau" layoutX="274.0" layoutY="100.0" onMouseClicked="#mouse" onMouseEntered="#afficher" prefHeight="200.0" prefWidth="244.0">
        <columns>
          <TableColumn fx:id="idcolumn" prefWidth="75.0" text="id" />
          <TableColumn fx:id="nomcolumn" prefWidth="75.0" text="nom" />
            <TableColumn fx:id="prixcolumn" prefWidth="93.0" text="prix" />
        </columns>
      </TableView>
      <TextField fx:id="idnom" layoutX="14.0" layoutY="108.0" onMouseClicked="#mouseclicked" promptText="nom du produit" />
      <TextField fx:id="prixid" layoutX="14.0" layoutY="164.0" promptText="prix" />
      <Button fx:id="ajouterid" layoutX="63.0" layoutY="323.0" mnemonicParsing="false" onAction="#ajouter" text="ajouter" />
      <Text fx:id="idtext" layoutX="242.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Button layoutX="127.0" layoutY="267.0" mnemonicParsing="false" onAction="#Supprimer" text="Supprimer" />
   </children>
</AnchorPane>
